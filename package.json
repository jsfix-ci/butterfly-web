{
  "name": "butterfly-web",
  "version": "0.1.2",
  "description": "The Butterfly Gradle plugin translates code from Kotlin to Typescript, but in order to do so, a set of libraries must be present on both sides.  This is the web portion.",
  "main": "Kotlin.js",
  "types": "Kotlin.d.ts",
  "dependencies": {
    "@types/mersenne-twister": "1.x",
    "@types/sprintf-js": "1.1.x",
    "@types/uuid": "8.x",
    "iterable-operator": "0.10.x",
    "mersenne-twister": "1.x",
    "rxjs": "6.x",
    "sprintf-js": "1.1.x",
    "ts-polyfill": "^3.8.2",
    "uuid": "8.x"
  },
  "peerDependencies": {},
  "devDependencies": {
    "typescript": "^3.4.5",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.1",
    "@types/jest": "^26.0.20"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --env=node --colors --coverage --passWithNoTests test"
  },
  "author": "Lightning Kite",
  "license": "MIT",
  "directories": {
    "lib": "src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lightningkite/butterfly-web.git"
  },
  "bugs": {
    "url": "https://github.com/lightningkite/butterfly-web/issues"
  },
  "homepage": "https://github.com/lightningkite/butterfly-web#readme"
